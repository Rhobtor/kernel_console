# BusyBox 1.36.1 Configuration
# Kernel Console v0.1

# Generated from: make defconfig
# Customized for embedded gaming console

# Build options
CONFIG_STATIC=y
CONFIG_SHARED=n

# Busybox configuration
CONFIG_FEATURE_VERBOSE_USAGE=y
CONFIG_FEATURE_EDITING=y
CONFIG_FEATURE_EDITING_HISTORY=50

# Applets to include (essential for console)
CONFIG_SH_IS_ASH=y
CONFIG_BASH_IS_NONE=y
CONFIG_ASH=y
CONFIG_ASH_ALIAS=y
CONFIG_ASH_GETOPTS=y

# File utilities
CONFIG_CP=y
CONFIG_MV=y
CONFIG_RM=y
CONFIG_MKDIR=y
CONFIG_RMDIR=y
CONFIG_LS=y
CONFIG_CAT=y
CONFIG_MORE=y
CONFIG_LESS=y
CONFIG_FIND=y
CONFIG_GREP=y
CONFIG_SORT=y
CONFIG_UNIQ=y
CONFIG_WC=y

# System utilities
CONFIG_MOUNT=y
CONFIG_UMOUNT=y
CONFIG_BLKID=y
CONFIG_FDISK=y
CONFIG_MODPROBE=m
CONFIG_INSMOD=m
CONFIG_LSMOD=m
CONFIG_LSUSB=m
CONFIG_LSPCI=m

# Process utilities
CONFIG_PS=y
CONFIG_TOP=y
CONFIG_PKILL=y
CONFIG_KILLALL=y
CONFIG_INIT=y

# Text processing
CONFIG_SED=y
CONFIG_AWK=y
CONFIG_PRINTF=y
CONFIG_ECHO=y
CONFIG_TEST=y

# Networking (basic, for future expansion)
CONFIG_WGET=y
CONFIG_TELNET=m

# Disable problematic modules for this build
# CONFIG_TC is not set
